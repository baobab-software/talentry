{
  "name": "@talentry/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types and interfaces for Talentry monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.mjs",
      "require": "./dist/common/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "types",
    "typescript",
    "shared",
    "interfaces"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.16.9",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}
